; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="net cliente 4"
run/main_scene="res://esenas/main_rcp/main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/image="res://assets/fondo/fondo-godot.png"
config/icon="res://assets/icon/icoe-net.png"

[autoload]

Data="*res://esenas/global/data.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[file_customization]

folder_colors={
"res://assets/": "green",
"res://assets/Buttons/": "green",
"res://assets/fondo/": "green",
"res://class/": "purple",
"res://esenas/": "red",
"res://pruebas/": "gray",
"res://resources/": "orange"
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
